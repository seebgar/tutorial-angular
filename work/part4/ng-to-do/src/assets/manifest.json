{
  "name": "Todo App",
  "description": "Todo app as chrome extension using angular",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "web_accessible_resources": [
    "assets/img/*",
    "assets/fonts/*"
  ]
}
